select RT.CPTDESC AS 'REFERRAL TYPE', RST.CPSTDESC AS 'REFERRAL SUBTYPE', R.NAME AS 'REFERRAL' from COMPREFERRAL R with (nolock)
left join COMPREFERRALTYPE RT with (nolock) on R.CPTID = RT.CPTID
left join COMPREFERRALSUBTYPE RST with (nolock) ON R.CPSTID = RST.CPSTID
ORDER BY RT.CPTDESC, RST.CPSTDESC
